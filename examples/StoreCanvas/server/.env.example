PORT=4000
NODE_ENV=development

# Linconwaves AI Workers (https://developers.linconwaves.com)
AIWORKERS_BASE_URL=https://aiworker.linconwaves.com
AIWORKER_API_KEY=replace-with-your-api-key
BACKGROUND_MODEL_ID=Stable Diffusion XL Base 1.0
IMG2IMG_MODEL_ID=Sd V1 5 Img2img
INPAINT_MODEL_ID=Sd V1 5 Inpainting
STYLE_LLM_MODEL_ID=Llama 3.1 8B Instruct

# Database provider: mongodb | postgres | supabase
DB_PROVIDER=mongodb
MONGODB_URI=mongodb://127.0.0.1:27017/storecanvas
# DATABASE_URL=postgres://user:password@localhost:5432/storecanvas
# SUPABASE_DB_URL=postgres://user:password@host:5432/postgres

# Storage provider: supabase | cloudflare_r2 | aws_s3
STORAGE_PROVIDER=supabase
SUPABASE_STORAGE_BUCKET=storecanvas-assets
SUPABASE_STORAGE_URL_BASE=https://your-project-id.supabase.co/storage/v1/object/public/storecanvas-assets
SUPABASE_SERVICE_ROLE_KEY=replace-with-supabase-service-role-key
# R2_ACCOUNT_ID=your-r2-account-id
# R2_ACCESS_KEY_ID=your-r2-access-key
# R2_SECRET_ACCESS_KEY=your-r2-secret
# R2_BUCKET=storecanvas-assets
# R2_PUBLIC_BASE_URL=https://pub-1234.r2.dev
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-aws-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret
# AWS_S3_BUCKET=storecanvas-assets
# AWS_S3_PUBLIC_BASE_URL=https://your-cdn.example.com

SESSION_SECRET=replace-with-random-string
JWT_SECRET=replace-with-long-random-string

RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Optional email (Resend)
# RESEND_API_KEY=
# RESEND_FROM_EMAIL=no-reply@example.com
